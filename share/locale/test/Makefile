MSGDIR=		${SHAREDIR}/locale/zz_AA.UTF-8/LC_MESSAGES
SYMLINKS+=	../en_US.UTF-8/LC_COLLATE ${MSGDIR:H}/LC_COLLATE
SYMLINKS+=	../en_US.UTF-8/LC_CTYPE ${MSGDIR:H}/LC_CTYPE
SYMLINKS+=	../en_US.UTF-8/LC_MONETARY ${MSGDIR:H}/LC_MONETARY
SYMLINKS+=	../en_US.UTF-8/LC_NUMERIC ${MSGDIR:H}/LC_NUMERIC
SYMLINKS+=	../en_US.UTF-8/LC_TIME ${MSGDIR:H}/LC_TIME
SYMLINKS+=	../../en_US.UTF-8/LC_MESSAGES/LCL_DATA ${MSGDIR}/LCL_DATA

all:	SUNW_OST_OSLIB.mo
CLEANFILES+=	SUNW_OST_OSLIB.mo
.SUFFIXES: .po .mo
.po.mo:
	msgfmt -o $@ $<

proginstall:
	${INSTALL} -d ${PROG_INSTALL_OWN} -m 755 \
	    ${DESTDIR}${MSGDIR}
	${INSTALL} ${COPY} ${PROG_INSTALL_OWN} -m ${NONBINMODE} \
	    ${.CURDIR}/priv_names SUNW_OST_OSLIB.mo \
	    ${DESTDIR}${MSGDIR}/

.include <bsd.prog.mk>
