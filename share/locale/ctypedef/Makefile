FILES=		${LOCALES:S/$/.LC_CTYPE/}
CLEANFILES=	${FILES}

.for f t in ${SAME}
SYMLINKS+=	../$f/${FILESNAME} \
    ${LOCALEDIR}/$t/${FILESNAME}
.endfor

.for f in ${LOCALES}
FILESDIR_${f}.LC_CTYPE= ${LOCALEDIR}/${f}
.endfor

SYMPAIRS+=	ru_RU.ISO8859-5.src ru_RU.KOI8-R.src
SYMPAIRS+=	en_US.ISO8859-1.src en_US.ISO8859-15.src
SYMPAIRS+=	en_US.ISO8859-1.src lv_LV.ISO8859-13.src
SYMPAIRS+=	en_US.ISO8859-1.src sq_AL.ISO8859-2.src
SYMPAIRS+=	en_US.ISO8859-1.src tr_TR.ISO8859-9.src

.for s t in ${SYMPAIRS}
${t:S/src$/LC_CTYPE/}: $s
	localedef -D -U -c -w ${MAPLOC}/widths.txt \
	-f ${MAPLOC}/map.${.TARGET:T:R:C/^.*\.//} \
	-i ${.ALLSRC} ${.OBJDIR}/${.TARGET:T:R} 
.endfor

# since we don't have bsd.files.mk, we need to handle FILES here
all: ${FILES}
proginstall:
.for f in ${FILES}
	[ -d ${DESTDIR}${LOCALEDIR}/${f:R}/${FILESNAME} ] || \
	    ${INSTALL} -d ${PROG_INSTALL_OWN} -m 755 \
	    ${DESTDIR}${LOCALEDIR}/${f:R}/${FILESNAME}
	${INSTALL} ${COPY} ${PROG_INSTALL_OWN} -m ${NONBINMODE} \
	    ${f} ${DESTDIR}${LOCALEDIR}/${f:R}/${FILESNAME}/LCL_DATA
.endfor
.include <bsd.prog.mk>
